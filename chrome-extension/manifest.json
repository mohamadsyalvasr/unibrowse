{
  "manifest_version": 3,
  "name": "Local Browser Sync Client",
  "version": "0.2",
  "description": "Sync bookmarks ke server lokal FastAPI.",
  "permissions": [
    "bookmarks",
    "storage",
    "alarms"
  ],
  "host_permissions": [
    "http://127.0.0.1:8000/*"
  ],
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_title": "Sync Browser Data",
    "default_popup": "popup.html"
  }
}
